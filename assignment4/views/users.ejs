<%- include('includes/head.ejs') %>

</head>


<body>
    
    <%- include('includes/navigation.ejs') %>


    <% if (prods.length > 0) { %>
        <div class="users">
            <header>
                <h1 style="margin-left: 1rem;"> Registered Users </h1>
            </header>
            <ul class="users-list">
                <% for (let item of prods) { %>
                    <li> <%= item.username %> </li>
                <% } %>
            </ul>
        </div>
    <% } else { %>
        <p style="margin-left: 1rem; font-family: sans-serif; font-size: 1.5rem;"> No Users </p>
    <% } %>
</body>
</html>